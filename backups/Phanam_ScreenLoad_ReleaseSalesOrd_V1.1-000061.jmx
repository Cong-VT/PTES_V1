          <stringProp name="query">CREATE TABLE #temp
	(
		INDEXT INT IDENTITY(1,1)
		,PackageID VARCHAR(100)
	)

CREATE TABLE #tempLot
	(
		INDEXT INT IDENTITY(1,1)
		,PackageID VARCHAR(100)
		,LotSerNbr [varchar] (50)
	)

INSERT INTO #temp
SELECT PackageID 
FROM API_StormPack WHERE InvtID=&apos;${InvtID2}&apos;
DECLARE @PackageIDUsed NVARCHAR(100)
DECLARE @LotUnUsed VARCHAR(50)
DECLARE @flag INT =1
DECLARE @endflag INT 
SELECT @endflag=MAX(INDEXT) FROM #temp
WHILE (@flag&lt;=@endflag)
	BEGIN
		DECLARE @PackageID VARCHAR(100)
		SELECT @PackageID=PackageID FROM #temp WHERE INDEXT=@flag
		DECLARE @flagLot INT=1

		INSERT INTO #tempLot
		SELECT PackageID ,LotSerNbr
		FROM dbo.API_StormLot WHERE PackageID=@PackageID
		DECLARE @endflaglot INT 
		DECLARE @QtyLotOfPack INT
		DECLARE @QtyLotOfPackScaned INT
		SELECT @endflaglot=MAX(INDEXT) FROM #tempLot		
		WHILE (@flagLot&lt;= @endflaglot)
				BEGIN
					SELECT @QtyLotOfPack=COUNT(LotSerNbr) FROM API_StormLot WHERE PackageID=@PackageID GROUP BY PackageID
					SELECT @QtyLotOfPackScaned=COUNT(LotSerNbr) FROM dbo.OM_PDALotTrans WHERE LotSerNbr IN (SELECT LotSerNbr FROM API_StormLot WHERE PackageID=@PackageID )
					IF ((@QtyLotOfPack=@QtyLotOfPackScaned) OR (@QtyLotOfPackScaned=0))
						GOTO NextPack ;
						ELSE
						IF ((@QtyLotOfPack &lt;&gt; @QtyLotOfPackScaned) AND (@QtyLotOfPackScaned&lt;&gt;0) )
							BEGIN	
							SELECT @PackageIDUsed=@PackageID	
							GOTO Finded ;
							END								
				END

			NextPack:
			SET @flag =@flag+1
		 

	end

Finded:
SELECT @PackageIDUsed AS PackageIDScaned

SELECT @LotUnUsed=LotSerNbr FROM  dbo.API_StormLot WHERE PackageID=@PackageIDUsed
AND LotSerNbr NOT IN (SELECT LotSerNbr FROM dbo.OM_PDALotTrans	 WHERE LotSerNbr IN (SELECT LotSerNbr FROM API_StormLot WHERE PackageID=@PackageIDUsed))

SELECT @LotUnUsed AS LotUnUsed

DROP TABLE #temp
DROP TABLE #tempLot</stringProp>
