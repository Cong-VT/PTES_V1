DECLARE @site NVARCHAR(30)=&apos;100043&apos;
DECLARE @MnDate NVARCHAR(30)=&apos;2019/11/09&apos;
DECLARE @expDate NVARCHAR(30)=&apos;2030/11/08&apos;
CREATE TABLE #Invttemp1 (
					ID INT IDENTITY(1,1)
					,InvtID NVARCHAR(30)
					,CnvFact INT
					)
INSERT INTO #Invttemp1
SELECT InvtID , CnvFact
FROM dbo.IN_Inventory
WHERE InvtID IN (&apos;TDT0377&apos;,&apos;TDT0397&apos;)

DECLARE @KeyTEXT NVARCHAR(10) 
DECLARE @SKU INT=${QtySKU}----------------------------- Bat buoc
DECLARE @QtyPac INT=${QtyPACScan} ----------------------------- Bat buoc
DECLARE @SSKU INT=1
DECLARE @time NVARCHAR(30)
DECLARE @prefixbytime NVARCHAR(10)
SELECT @time=convert(varchar, getdate(), 108)
SELECT @KeyTEXT=SUBSTRING(@time,1,2)+SUBSTRING(@time,4,2)+SUBSTRING(@time,7,2)
--SELECT @KeyTEXT
WHILE (@SSKU &lt;=@SKU)
BEGIN
	DECLARE @Invt NVARCHAR(30)
	DECLARE @CnvFac INT
	SELECT @Invt=InvtID FROM #Invttemp1 WHERE ID=@SSKU	
	SELECT @CnvFac=CnvFact FROM #Invttemp1 WHERE ID=@SSKU		
	DECLARE @QtyofInvt INT =3 -- Số sản phẩm 

	--------------@Invt_TDT0372

	DECLARE @PrefixPac NVARCHAR(50)=&apos;PTeSR1_&apos;+@Invt + CAST( CONVERT(varchar, GETDATE(), 112) AS NVARCHAR(30))  +&apos;_&apos; + @KeyTEXT
	--DECLARE @PrefixPac NVARCHAR(50)=&apos;PTeSR1_&apos;+@Invt + CAST( CONVERT(varchar, GETDATE(), 112) AS NVARCHAR(30))  +&apos;_&apos; +  CAST( CONVERT(varchar, GETDATE(), 108) AS NVARCHAR(30))
	DECLARE @flag INT=1
	DECLARE @endpoit INT=100
	DECLARE @SetInvtID NVARCHAR(30)
	--SELECT TOP 1 @SetInvtID =InvtID FROM #Temp WHERE StatusI=&apos;NY&apos;

	SELECT @flag=1 
	FROM #Invttemp1 WHERE InvtID=@SetInvtID

	WHILE (@flag&lt;=@QtyPac)
		BEGIN	
			DECLARE @PackageID NVARCHAR(100)
			DECLARE @KGPACSt FLOAT
			DECLARE @KGPAC FLOAT
			DECLARE @t FLOAT
			SELECT @t= (SELECT RAND() Result);
			SELECT @KGPACSt=CnvFact FROM dbo.IN_UnitConversion WHERE InvtID=@Invt AND FromUnit=&apos;PAC&apos;
			SELECT @PackageID = @PrefixPac+CAST(CAST(GETDATE() AS DATE) AS NVARCHAR(20))+&apos;00&apos;+CAST(@flag AS NVARCHAR(3))
			SELECT @KGPAC=@KGPACSt+CAST(RIGHT(@t,2) AS FLOAT)
		
			INSERT INTO dbo.API_StormPack(PackageID, MFGDate, Factory, InvtID, PackWt, Crtd_DateTime, Crtd_Prog, Crtd_User, LUpd_DateTime, LUpd_Prog, LUpd_User) 
			VALUES(@PackageID, &apos;2017-04-30&apos;, N&apos;NM1&apos;, @Invt, @KGPAC, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;)

			DECLARE @QtyLOT INT
			DECLARE @flagLOT INT=1

			WHILE (@flagLOT&lt;=@CnvFac)
				BEGIN	
					DECLARE @LotSerNbr NVARCHAR(50)
					DECLARE @KGLOTSt FLOAT
					DECLARE @KGLOT FLOAT
					DECLARE @tLot FLOAT
					SELECT @t= (SELECT RAND() Result);
					SELECT @KGLOTSt=CnvFact FROM dbo.IN_UnitConversion WHERE InvtID=@Invt AND FromUnit=&apos;LOT&apos;
					SELECT @LotSerNbr = @PrefixPac+CAST(CAST(GETDATE() AS DATE) AS NVARCHAR(20)) +&apos;00&apos;+CAST(@flag AS NVARCHAR(3))+CAST(@flagLOT AS NVARCHAR(3))
					SELECT @KGLOT=@KGLOTSt+CAST(RIGHT(@t,2) AS FLOAT)				
					
						INSERT INTO dbo.API_StormLot(PackageID, LotSerNbr, StkWt, returned, Crtd_DateTime, Crtd_Prog, Crtd_User, LUpd_DateTime, LUpd_Prog, LUpd_User) 
						VALUES(@PackageID, @LotSerNbr, @KGLOT, 0, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;)
				
					SET @flagLOT=@flagLOT+1

				end
	
	
					-- Next Row
					SET @flag=@flag+1				
	END
	--INSERT INTO dbo.API_StormPack(PackageID, MFGDate, Factory, InvtID, PackWt, Crtd_DateTime, Crtd_Prog, Crtd_User, LUpd_DateTime, LUpd_Prog, LUpd_User) 
	--VALUES(N&apos;DK64&apos;, &apos;2018-04-30&apos;, N&apos;NM1&apos;, N&apos;TDT0397&apos;, 40.6, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;, GETDATE(), N&apos;PTeS&apos;, N&apos;PTeS&apos;)
	SET @SSKU=@SSKU+1
END

DECLARE @strpack NVARCHAR(100)
SELECT @strpack=&apos;%&apos;+@KeyTEXT+&apos;%&apos;
SELECT *
INTO #temp1
FROM dbo.API_StormPack 
WHERE PackageID LIKE @strpack

SELECT b.InvtID,a.PackageID,a.LotSerNbr,a.StkWt
INTO #temp2
FROM dbo.API_StormLot a
INNER JOIN #temp1 b ON b.PackageID = a.PackageID
WHERE a.PackageID LIKE @strpack



SELECT  DISTINCT PackageID FROM #temp2 a

SELECT DISTINCT  
	a.InvtID
	,invt.Descr
	,SiteID=&apos;100043&apos;
	,Unit=&apos;KG&apos;
	,QTY=c.StkWt
	,CAST(9999 AS FLOAT) AS Price
	,CAST(9999 AS FLOAT) AS TotalAmt
	,c.LotSerNbr
	,@MnDate AS NSX
	,@expDate AS NHH
	,a.PackageID	

FROM dbo.#temp2 a
INNER JOIN dbo.API_StormLot c ON c.PackageID = a.PackageID
INNER JOIN dbo.IN_Inventory invt(NOLOCK) ON invt.InvtID = a.InvtID
